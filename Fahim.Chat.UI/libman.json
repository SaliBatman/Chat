{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "filesystem",
      "library": "wwwroot/js/",
      "destination": "wwwroot/lib/js/",
      "files": [
        "signup.js",
        "site.js",
        "site.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-steps@1.1.0",
      "destination": "wwwroot/lib/jquery-steps/"
    }
  ]
}